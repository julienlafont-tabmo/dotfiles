# GIT
alias go='git checkout'
alias ga='git add'
alias gd='git diff'
alias gdc='git diff'
alias gs='git status'

# SSH
alias sshperso='ssh julien@pc.studio-dev.fr'
alias sshdev-manager-api='ssh vm@dev-manager-api'
alias sshdev-manager-db='ssh postgresql@dev-manager-db'

# Util
alias reload-alias='source ~/.config/.alias'

# Docker
alias dcker-clean-images='docker images | awk \'{print "docker rmi -f "$3}\' | tail -n+2 | sh'
alias dcker-clean='docker rm (docker ps -a -q)'

# Touchpad
alias on='notify-send "Touchpad Actif"; synclient TouchpadOff=0'
alias off='notify-send "Touchpad Désactivé"; synclient TouchpadOff=1'
alias keyboardfix='ibus-daemon -rd'
