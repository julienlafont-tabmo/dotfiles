#studiodev account
Host studiodev.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_studiodev

#tabmo account
Host work.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host *
  ServerAliveInterval 240
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
