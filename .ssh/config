#studiodev account
Host studiodev.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_studiodev

#tabmo account
Host work.github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

### BEGIN ovh MARKUP
# heatbeat SSH connexion
Host *
    ServerAliveCountMax 4
    ServerAliveInterval 15

# Ovh dev tunnel
Host ovh
    Hostname 151.80.187.218
    Port 22
    User vm

    # cf hosts.ovh
    LocalForward localhost:80 151.80.187.218:80

    # Jenkins (reverse proxy from sirius)
    LocalForward localhost:8080 151.80.187.218:8080

    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

### END ovh MARKUP
### BEGIN tabmo.priv MARKUP
# heatbeat SSH connexion
Host *
    ServerAliveCountMax 4
    ServerAliveInterval 15

Host tabmo.priv
    Hostname 149.202.203.118
    Port 22
    User ubuntu

    # cf hosts.jenkins
    LocalForward localhost:80 10.75.17.198:80

    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

### END tabmo.priv MARKUP
